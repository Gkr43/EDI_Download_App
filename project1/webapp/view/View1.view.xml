<mvc:View
xmlns:mvc="sap.ui.core.mvc"
xmlns="sap.m"
xmlns:core="sap.ui.core"
xmlns:tnt="sap.tnt" controllerName="project1.controller.View1" displayBlock="true">
<!--  ToolHeader for the app's title and navigation  -->
<tnt:ToolHeader id="shellLikeToolHeader" class="sapUiTinyMargin">
	<!--  <Button id="back" icon="sap-icon://nav-back" press="onBack" visible="true" tooltip="ZurÃ¼ck"/>  -->
	<Image id="_IDGenImage1" src="https://sapui5.hana.ondemand.com/resources/sap/ushell/themes/base/img/SAPLogo.svg" decorative="false" press="onLogoPressed" tooltip="SAP Logo">
		<layoutData>
			<OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData1" priority="NeverOverflow"/>
		</layoutData>
	</Image>
	<Title text="GATE ENTRY GENERATOR" wrapping="false" id="productName">
		<layoutData>
			<OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData2" priority="Disappear"/>
		</layoutData>
	</Title>
	<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
	<ToolbarSpacer id="spacer">
		<layoutData>
			<OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData3" priority="Low" group="1"/>
		</layoutData>
	</ToolbarSpacer>
	<tnt:ToolHeaderUtilitySeparator id="_IDGenToolHeaderUtilitySeparator1"/>
	<Avatar id="_IDGenAvatar1" src="/home/user/projects/adgroup_management_2/resources/image.png" displaySize="XS" press=".onAvatarPressed" tooltip="Profile">
		<layoutData>
			<OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData4" priority="NeverOverflow"/>
		</layoutData>
	</Avatar>
</tnt:ToolHeader>
<!--  Main Application Content  -->
<App id="appContainer">
	<Page id="mainPage" title="EDI GATE ENTRY">
		<content>
			<!--  Table  -->
			<Table id="table" inset="false">
				<columns>
					<Column id="_IDGenColumn1">
						<Text id="_IDGenText1" text="Field"/>
					</Column>
					<Column id="_IDGenColumn2">
						<Text id="_IDGenText2" text="Value"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="_IDGenColumnListItem1">
						<cells>
							<Text id="_IDGenText3" text="EDI Number"/>
							<Input id="ediNumberInput" placeholder="Enter EDI Number"/>
						</cells>
					</ColumnListItem>
					<ColumnListItem id="_IDGenColumnListItem2">
						<cells>
							<Text id="_IDGenText4" text="Plant"/>
							<Input id="plantInput" placeholder="Enter Plant"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
			<!--  Button moved to the bottom  -->
			<Button id="generateGateEntryButton" text="Generate Gate Entry" press="onGenerateGateEntry" type="Emphasized" class="sapUiTinyMarginTop sapUiTinyMarginBottom"/>
		</content>
	</Page>
</App>undefined</mvc:View>